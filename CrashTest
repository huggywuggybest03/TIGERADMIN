rs = game:GetService("RunService")
rs.Heartbeat:Connect(function()
	pcall(function()
		if _G.spamming2 == true and game:FindFirstChild("Tiger_Loaded") then

			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)

			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)

			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)
			game.Workspace.Remote.arrest:InvokeServer(game.Players[_G.SpammingPlrcrash].Character.Head)

		end
	end)
end)
